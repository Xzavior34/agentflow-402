# AgentMarket Deployment Configuration
# Copy this to .env and fill in your values

# Required: Your deployer wallet private key (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# Optional: Contract address (auto-populated after deployment)
CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000

# Optional: RPC URL override
RPC_URL=https://testnet.zkevm.cronos.org

# Optional: Payment amount for simulation (in TCRO)
PAYMENT_AMOUNT=0.0001

# Optional: Cronos API key for contract verification
CRONOS_API_KEY=
